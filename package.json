{
  "name": "jslide",
  "version": "0.0.1",
  "description": "JS/HTML5/CSS3 presentation maker.",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel index.html --out-dir ./.dist",
    "build": "parcel build index.js --no-content-hash --public-url . --out-dir ./dist index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Viglino/jslide.git"
  },
  "keywords": [
    "slideshow"
  ],
  "author": "Jean-Marc Viglino (https://github.com/Viglino)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Viglino/jslide/issues"
  },
  "homepage": "https://github.com/Viglino/jslide#,",
  "dependencies": {
    "file-saver": "^2.0.1",
    "highlight.js": "^10.1.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.0.0"
  },
  "staticFiles": {
    "staticPath": [
      "assets"
    ],
    "watcherGlob": "**"
  }
}
